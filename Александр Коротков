  ._hfixed .lm_anim #l, ._hfixed .lm_anim #m .mhead { transition: none; }
  ._hfixed .lm_anim_end #l, ._hfixed .lm_anim_end #m .mhead, ._hfixed .lm_anim_start #l, ._hfixed .lm_anim_start #m .mhead { transform: translate3d(0px, 0px, 0px); }
}

@media screen and (min-width: 882px) {
