.Unfold { position: relative; user-select: none; }

.Unfold__popup { min-width: 100px; position: absolute; z-index: 10; left: 4px; top: 4px; background: rgb(255, 255, 255); overflow-x: hidden; border-radius: 2px; border: 1px solid rgb(222, 224, 227); display: none; overflow-y: auto; }

.vk_js_no .Unfold__popup { top: 43px; }

.Unfold_open .Unfold__popup { display: block; }

.Unfold_position_topRight .Unfold__popup { left: auto; right: 4px; }

.Unfold_position_bottomRight .Unfold__popup { left: auto; right: 4px; bottom: 4px; top: auto; }

.Unfold_position_bottomLeft .Unfold__popup { bottom: 4px; top: auto; }

@supports ((-webkit-box-shadow:initial) or (-moz-box-shadow:initial) or (box-shadow:initial)) {
  .Unfold__popup { border: none; box-shadow: rgba(0, 0, 0, 0.14) 0px 1px 3px 1px; }
}

@supports ((-webkit-transition:initial) or (-moz-transition:initial) or (-o-transition:initial) or (transition:initial)) {
  .Unfold__popup { display: block; visibility: hidden; opacity: 0; transform: translateY(-10px); transition: visibility 0s linear 0.2s, transform 0.2s ease, opacity 0.2s ease, -webkit-transform 0.2s ease, -moz-transform 0.2s ease, -o-transform 0.2s ease; }
  .Unfold_position_bottomLeft .Unfold__popup, .Unfold_position_bottomRight .Unfold__popup { transform: translateY(10px); }
  .Unfold_open .Unfold__popup { visibility: visible; opacity: 1; transform: translateY(0px); transition-delay: 0s; }
}

.Unfold__trigger { -webkit-tap-highlight-color: transparent; cursor: pointer; color: rgb(255, 255, 255); font-size: 16px; font-weight: 500; overflow: hidden; display: flex; white-space: nowrap; min-width: 44px; min-height: 44px; height: 100%; text-decoration: none !important; }

